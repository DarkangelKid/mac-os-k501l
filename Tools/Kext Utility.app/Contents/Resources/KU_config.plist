<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>AddBootArgs</key>
	<dict>
		<key>-v</key>
		<false/>
		<key>kext-dev-mode=1</key>
		<true/>
		<key>rootless=0</key>
		<false/>
	</dict>
	<key>PatchKexts</key>
	<dict>
		<key>AppleKextExcludeList.kext</key>
		<true/>
		<key>msdosfs.kext</key>
		<true/>
	</dict>
</dict>
</plist>
